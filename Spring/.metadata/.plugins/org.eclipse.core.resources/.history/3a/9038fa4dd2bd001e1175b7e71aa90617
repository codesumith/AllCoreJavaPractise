package com.sumith.order;

import org.springframework.context.annotation.Configuration;


@Configuration
public class SpringBeanConfiguration {

	
	public Order createOrderOne()
	{
		Order o1= new Order();
		o1.setoId(123);
		o1.setPrice(2000.0);
		
		return o1;
	}
	
	
	public Products createProductOne()
	{
		return new Products();
	}
	
}
