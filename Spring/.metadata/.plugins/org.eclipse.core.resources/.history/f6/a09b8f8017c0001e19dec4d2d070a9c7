package com.flipkart;

import org.springframework.stereotype.Component;

@Component
public class Product {

	
	public int getPid() {
		return pid;
	}

	public void setPid(int pid) {
		this.pid = pid;
	}

	private int pid;
	
	public Product() {
		System.out.println("Product is instantiated...");
	}

}
