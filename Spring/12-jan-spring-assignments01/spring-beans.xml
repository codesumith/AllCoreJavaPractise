<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<!--private Integer id; private String name; private Map<String,Integer> marks; -->
        
        <beans>
              <bean id="student1" class="com.student.details.Student">
              <property name="id" value="123"></property>
               <property name="name" value="Arjun"></property>
                <property name="marks" >
                <map>
               
                <entry key="java" value="88"></entry>
                  <entry key="Python" value="77"></entry>
                    <entry key="C++" value="90"></entry>
                      <entry key="Java" value="92"></entry>
                
                </map>
                </property>
              
              </bean>
              
              
          <bean id="student2" class="com.student.details.Student">
          <constructor-arg value="321"></constructor-arg>
          <constructor-arg value="Rama"></constructor-arg>
          <constructor-arg>
          <map>
                    <entry key="Css" value="78"></entry>
                    <entry key="C" value="68"></entry>
                    <entry key="JavaScript" value="76"></entry>
          
          
          </map>
          </constructor-arg>
          
          </bean>
        
        </beans>